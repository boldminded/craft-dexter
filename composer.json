{
  "name": "boldminded/craft-dexter",
  "description": "Advanced Algolia or Meilisearch index management and search",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "craft-plugin",
  "license": "proprietary",
  "support": {
    "email": "support@boldminded.com",
    "issues": "https://github.com/git@github.com:litzinger/craft-dexter.git/issues?state=open",
    "source": "https://github.com/git@github.com:litzinger/craft-dexter.git",
    "docs": "https://github.com/git@github.com:litzinger/craft-dexter.git",
    "rss": "https://github.com/git@github.com:litzinger/craft-dexter.git/releases.atom"
  },
  "require": {
    "php": ">=8.2",
    "craftcms/cms": "^5.8.0",
    "litzinger/dexter-core": "^1.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:litzinger/dexter-core.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "boldminded\\dexter\\": "src/"
    }
  },
  "extra": {
    "handle": "dexter",
    "name": "Dexter",
    "developer": "BoldMinded",
    "documentationUrl": "https://github.com/git@github.com:litzinger/craft-dexter.git",
    "class": "boldminded\\dexter\\Dexter"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "php-http/discovery": true,
      "yiisoft/yii2-composer": true
    }
  }
}
