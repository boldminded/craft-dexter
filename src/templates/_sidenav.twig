<nav>
    <ul>
        {% for handle, item in navItems %}
            {% if item.heading is defined %}
                <li class="heading"><span>{{ item.heading }}</span></li>
            {% else %}
                <li>
                    {% if item.external is defined and item.external %}
                        <a href="{{ handle }}" target="_blank" rel="noopener noreferrer">{{ item.title }}</a>
                    {% else %}
                        <a href="{{ url('dexter/'~handle) }}"{% if handle == selectedNavItem %} class="sel"{% endif %}>{{ item.title }}</a>
                    {% endif %}
                </li>
            {% endif %}
        {% endfor %}
    </ul>
</nav>
